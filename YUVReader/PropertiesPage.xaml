<Window x:Class="YUVReader.PropertiesPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:YUVReader"
        mc:Ignorable="d"
        Title="PropertiesPage" Height="450" Width="800">
    <Grid>
        <ListBox HorizontalAlignment="Left" Height="46" Margin="200,90,0,0" VerticalAlignment="Top" Width="300"/>
        <TextBox x:Name="Text_Box" HorizontalAlignment="Left" Height="23" Margin="200,140,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="125"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="200,170,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="125"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="330,141,0,0" VerticalAlignment="Top" Width="125"/>
    </Grid>
</Window>
